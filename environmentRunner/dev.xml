<?xml version="1.0" encoding="UTF-8"?>
<!-- // suite name có thể chuyển thành project name , name = "Suite" -> name 
	= "Nopcommerce" , parallel bỏ đi hoặc để đó cũng đc -->

<suite parallel="false" name="NopCommerce">


	<listeners>
		<listener class-name="common.MethodListener" />
	</listeners>
	
		<!-- Global name environment -->
	<parameter name="url" value="https://demo.nopcommerce.com/" />
	<parameter name="db.host" value="https://demo.nopcommerce.com/" />
	<parameter name="db.user" value="https://demo.nopcommerce.com/" />
	<parameter name="db.pass" value="https://demo.nopcommerce.com/" />
	
	<test name="Run On Chrome">
	
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.nopcommerce.user.Level_21_Multiple_Environment" />
		</classes>
		
	</test>
	
</suite> <!-- Suite -->
